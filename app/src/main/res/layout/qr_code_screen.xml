<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/qrCodeScreen"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFF"
    tools:context=".signIn_screen">

    <ImageView
        android:id="@+id/qrCodeScreen_menuBlackBackground"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/menu_bar_background"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="643dp" />

    <ImageButton
        android:id="@+id/qrCodeScreen_statusIcon"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:backgroundTint="#000000"
        app:srcCompat="@drawable/figma_white_clock_icon"
        tools:layout_editor_absoluteX="233dp"
        tools:layout_editor_absoluteY="668dp" />

    <ImageButton
        android:id="@+id/qrCodeScreen_hourLogIcon"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:backgroundTint="#000000"
        app:srcCompat="@drawable/figma_white_doc_icon"
        tools:layout_editor_absoluteX="330dp"
        tools:layout_editor_absoluteY="666dp" />

    <TextView
        android:id="@+id/qrCodeScreen_placeholderText"
        android:layout_width="274dp"
        android:layout_height="82dp"
        android:layout_marginStart="88dp"
        android:layout_marginTop="208dp"
        android:text="QR Code and Check mark Placed Here"
        android:textAppearance="@style/TextAppearance.AppCompat.Large"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageButton
        android:id="@+id/qrCodeScreen_QRCodeIcon"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:backgroundTint="#000000"
        app:srcCompat="@drawable/figma_green_qr_icon"
        tools:layout_editor_absoluteX="135dp"
        tools:layout_editor_absoluteY="668dp" />

    <ImageButton
        android:id="@+id/qrCodeScreen_homeIcon"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:backgroundTint="#000000"
        app:srcCompat="@drawable/figma_white_home_icon"
        tools:layout_editor_absoluteX="35dp"
        tools:layout_editor_absoluteY="666dp" />

</androidx.constraintlayout.widget.ConstraintLayout>